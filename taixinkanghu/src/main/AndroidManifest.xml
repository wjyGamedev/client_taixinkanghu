<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taixinkanghu">

    <!-- 用户权限 -->


    <!-- 下载列表 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- 屏幕适配 -->
    <!-- 程序入口 -->
    <application
        android:name="com.taixinkanghu.app.frame.AppFrame"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.taixinkanghu.app.ui.activity.WelcomeActivity"
            android:label="@string/welcome_activity_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.taixinkanghu.app.ui.activity.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>

        <service
            android:name="com.taixinkanghu.app.model.net.NetService"
            android:enabled="true"
            android:exported="true"></service>

        <activity
            android:name="com.taixinkanghu.app.ui.activity.AboutUsActivity"
            android:label="AboutUsActivity"></activity>
        <activity
            android:name="com.taixinkanghu.app.ui.activity.ChooseWorkerActivity"
            android:label="ChooseWorkerActivity"></activity>
        <activity
            android:name="com.taixinkanghu.app.ui.activity.CustomerServiceActivity"
            android:label="CustomerServiceActivity"></activity>
        <activity
            android:name="com.taixinkanghu.app.ui.activity.SaleInfoActivity"
            android:label="SaleInfoActivity"></activity>
        <activity
            android:name="com.taixinkanghu.app.ui.activity.WorkerInfoActivity"
            android:label="WorkerInfoActivity"></activity>
        <activity
            android:name="com.taixinkanghu.app.ui.activity.AppointmentActivity"
            android:label="AppointmentActivity"></activity>
        <activity
            android:name="com.taixinkanghu.app.ui.activity.OrderConfirmActivity"
            android:label="OrderConfirmActivity"></activity>
    </application>

</manifest>
